        </div>
        @using (Html.BeginForm("saleskayit", "Sales", FormMethod.Post))
        {
            <form class="form gr-form" style="margin-top: 2vw; margin-bottom: 2vw; margin-right: 2vw; margin-left: 2vw;">
                <div class="form-row">
                    <div class="col-md-6">
                        <div class="form-group gr-form-group gr-form-group-dark gr-form-group-inside" style="margin-top: 1vw; ">
